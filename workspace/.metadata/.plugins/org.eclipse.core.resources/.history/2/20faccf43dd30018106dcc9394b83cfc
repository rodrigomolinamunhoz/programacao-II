package exemplo;

public class Cadastro {

	public static void main(String[] args) {
		//Leitura do txt
		
		
		//Programa
		int opcao = 0;
		do {
			System.out.println("1 - novo carro");
			System.out.println("2 - listar carro");
			System.out.println("3 - sair");
		} while (opcao != 3);
		
		//Salvar no txt
	}

}
